<div>
    <h1>Hola, bienvenidos a Handlebars Store</h1>
  <h2>Listado de productos en stock</h2>
  <div class="rowencabezado">
    <p>ID</p><p>Nombre producto</p><p>Precio</p><p>Stock</p>
  </div>
    {{#each productosObtenidos}}
    <div class="row">
       <p>
        {{this.id}}
       </p> <p>{{this.title}}</p> <p> {{formatear this.price}} </p>
       <p>{{this.stock}} </p>
    </div>
    {{/each}}
   

</div>